# Shopify store domain (e.g., mystore.myshopify.com)
SHOP_NAME=your-shop.myshopify.com

# Shopify Admin API access token
ACCESS_TOKEN=your-access-token

# Numeric ID of the target location (e.g., 85972877548)
TARGET_LOCATION_ID=123456789

# Limit the number of products to process in a run (use None for all)
PRODUCT_LIMIT=5

# Only process items where inventory tracking is disabled (True/False)
ONLY_PROCESS_UNTRACKED=False
